        Clover.test("tuple test10", l.a == 1 && l.b == 2.0 && k == 2.1);

        (TupleData.c, TupleData.d) = (1,2);

        Clover.test("tuple test11", TupleData.c == 1 && TupleData.d == 2);
*/
    }
}
