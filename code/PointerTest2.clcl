class PointerClassB
{
    var_:int;

    def initialize(var_:int) {
        self.var_ = var_;
    }
}

class PointerClassA
{
    classVar: static int;
    var_:int;

    def initialize(): static {
        PointerClassA.classVar = 123;
    }

    def initialize() {
        self.var_ = 123;
    }

    def input_var(object:pointer):static {
        object->PointerClassB = new PointerClassB(234);
    }
}

class PointerTest2 {
    def main(): static {
        a:int = 123;

        b:pointer = &a;

        b->int = 234;

        Clover.test("pointer test1", a == 234);

        c:PointerClassA = new PointerClassA();

        Clover.test("pointer test2", PointerClassA.classVar == 123 && c.var_ == 123);

        d:pointer = &c.var_;

        d->int = 234;

        e:pointer = &PointerClassA.classVar;

        e->int = 234;

        Clover.test("pointer test2", PointerClassA.classVar == 234 && c.var_ == 234);

        f:PointerClassB = new PointerClassB(123);

        Clover.test("pointer test3", f.var_ == 123);

        PointerClassA.input_var(&f);

        Clover.test("pointer test4", f.var_ == 234);

        g:pointer = &f;

        Clover.test("pointer test5", g->PointerClassB.var_ == 234 && (&f)->PointerClassB.var_ == 234);

        (&f)->PointerClassB.var_ = 345;

        Clover.test("pointer test6", f.var_ == 345);
    }
}
